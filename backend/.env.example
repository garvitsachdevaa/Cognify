# Copy to .env and fill in your keys

# Supabase session-mode pooler URL (port 5432):
DATABASE_URL=postgresql+psycopg://postgres.[project-ref]:[password]@aws-0-ap-south-1.pooler.supabase.com:5432/postgres

GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-2.0-flash-lite

PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_INDEX_NAME=cognify

SUPERMEMORY_API_KEY=your_supermemory_api_key_here

TAVILY_API_KEY=your_tavily_api_key_here

# Space-separated list of allowed origins (add your Vercel URL in production):
CORS_ORIGINS=http://localhost:3000 https://your-app.vercel.app
